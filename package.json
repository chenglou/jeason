{
  "name": "flow-dev-tools",
  "version": "0.1.0",
  "dependencies": {
    "@opam-alpha/js_of_ocaml": "^2.8.2",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.8.0",
    "babel-register": "^6.8.0",
    "babylon": "^6.11.2",
    "blessed": "^0.1.81",
    "colors": "~1.1.2",
    "dependency-env": "github:npm-ml/dependency-env",
    "diff": "~2.2.1",
    "flow-parser": "~0.21.0",
    "glob": "^7.0.3",
    "minimist": "~1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "~2.0.0",
    "rimraf": "^2.5.2",
    "sane": "^1.4.0",
    "source-map-support": "~0.4.0",
    "nopam": "https://github.com/yunxing/nopam.git",
    "reason": "https://github.com/facebook/reason.git",
  	"@opam-alpha/merlin": "^ 2.5.0",
    "twit": "^2.1.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "syntax-flow",
      "syntax-trailing-function-commas",
      "transform-class-properties",
      "transform-flow-strip-types",
      "transform-regenerator"
    ]
  },
  "scripts": {
    "test": "make test",
    "start": "eval $(dependencyEnv) && nopam && make && echo '============================' && _build/src/main.native | refmt -use-stdin true -parse binary -print re -is-interface-pp false"
  },
  "devDependencies": {
  },
  "description": "Flow is a static typechecker for JavaScript. To find out more about Flow, check out [flowtype.org](http://flowtype.org/).",
  "main": "test.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenglou/flow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenglou/flow/issues"
  },
  "homepage": "https://github.com/chenglou/flow#readme"
}
