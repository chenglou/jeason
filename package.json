{
  "name": "jeason",
  "version": "0.35.0",
  "license": "BSD-3-Clause",
  "description": "The crappy js-to-reason converter anyone can contribute to!",
  "main": "main.re",
  "dependencies": {
    "reason": "https://github.com/facebook/reason.git"
  },
  "bin": {
    "jeason": "./jeason.sh"
  },
  "scripts": {
    "build": "rm -rf ./_build && ./node_modules/reason/_build/ocamlfind/bin/rebuild -no-links -package compiler-libs.common ./main.native",
    "start": "./jeason.sh ./test.js",
    "getReasonAst": "eval $(dependencyEnv) && nopam && ./node_modules/reason/refmt_impl.native -print ast ./getReasonAst.re"
  }
}
